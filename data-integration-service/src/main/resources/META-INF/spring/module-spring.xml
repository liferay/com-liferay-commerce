<?xml version="1.0"?>

<beans
	default-destroy-method="destroy"
	default-init-method="afterPropertiesSet"
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
>
	<bean class="com.liferay.data.integration.service.impl.HistoryLocalServiceImpl" id="com.liferay.data.integration.service.HistoryLocalService" />
	<bean class="com.liferay.data.integration.service.impl.HistoryServiceImpl" id="com.liferay.data.integration.service.HistoryService" />
	<bean class="com.liferay.data.integration.service.persistence.impl.HistoryPersistenceImpl" id="com.liferay.data.integration.service.persistence.HistoryPersistence" parent="basePersistence" />
	<bean class="com.liferay.data.integration.service.impl.ProcessLocalServiceImpl" id="com.liferay.data.integration.service.ProcessLocalService" />
	<bean class="com.liferay.data.integration.service.impl.ProcessServiceImpl" id="com.liferay.data.integration.service.ProcessService" />
	<bean class="com.liferay.data.integration.service.persistence.impl.ProcessPersistenceImpl" id="com.liferay.data.integration.service.persistence.ProcessPersistence" parent="basePersistence" />
	<bean class="com.liferay.data.integration.service.impl.ScheduledTaskLocalServiceImpl" id="com.liferay.data.integration.service.ScheduledTaskLocalService" />
	<bean class="com.liferay.data.integration.service.impl.ScheduledTaskServiceImpl" id="com.liferay.data.integration.service.ScheduledTaskService" />
	<bean class="com.liferay.data.integration.service.persistence.impl.ScheduledTaskPersistenceImpl" id="com.liferay.data.integration.service.persistence.ScheduledTaskPersistence" parent="basePersistence" />
</beans>