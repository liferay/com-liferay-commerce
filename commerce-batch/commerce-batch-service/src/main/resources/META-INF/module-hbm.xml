<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.liferay.commerce.batch.model.CommerceBatchJobExecution" />
	<import class="com.liferay.commerce.batch.model.CommerceBatchJobInstance" />
	<class name="com.liferay.commerce.batch.model.impl.CommerceBatchJobExecutionImpl" table="CommerceBatchJobExecution">
		<id name="commerceBatchJobExecutionId" type="long">
			<generator class="assigned" />
		</id>
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="commerceBatchJobInstanceId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="startTime" type="org.hibernate.type.TimestampType" />
		<property name="endTime" type="org.hibernate.type.TimestampType" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="exitCode" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="exitMessage" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.liferay.commerce.batch.model.impl.CommerceBatchJobInstanceImpl" table="CommerceBatchJobInstance">
		<id name="commerceBatchJobInstanceId" type="long">
			<generator class="assigned" />
		</id>
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="batchJobName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property column="key_" name="key" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>