<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder auto-namespace-tables="false" package-path="com.liferay.commerce.data.integration.manager">
	<namespace>DATA_INTEGRATION</namespace>
	<!--<entity data-source="sampleDataSource" local-service="true" name="Foo" remote-service="false" session-factory="sampleSessionFactory" table="foo" tx-manager="sampleTransactionManager uuid="true"">-->
	<entity local-service="true" name="History" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="historyId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="launchType" type="String" />
		<column name="scheduledTaskId" type="long" />
		<column name="status" type="int" />
		<column name="startDate" type="Date" />
		<column name="endDate" type="Date" />
		<column name="executionType" type="String" />
		<column name="errorLogFileEntryId" type="long" />
		<column name="runtimeLogFileEntryId" type="long" />

		<!-- Order -->

		<order by="desc">
			<order-column name="modifiedDate" />
		</order>

		<!-- Finder methods -->

		<finder name="GroupId" return-type="Collection">
			<finder-column name="groupId" />
		</finder>
		<finder name="GroupId_Status" return-type="Collection">
			<finder-column name="groupId" />
			<finder-column name="status" />
		</finder>
		<finder name="ScheduledTaskId" return-type="Collection">
			<finder-column name="companyId" />
			<finder-column name="scheduledTaskId" />
		</finder>
	</entity>
	<entity local-service="true" name="Process" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="processId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="name" type="String" />
		<column name="version" type="String" />
		<column name="className" type="String" />
		<column name="processType" type="String" />
		<column name="contextPropertiesFileEntryId" type="long" />
		<column name="srcArchiveFileEntryId" type="long" />

		<!-- Order -->

		<order by="desc">
			<order-column name="modifiedDate" />
		</order>

		<!-- Finder methods -->

		<finder name="GroupId" return-type="Collection">
			<finder-column name="groupId" />
		</finder>
		<finder name="CompanyId_Name" return-type="Process">
			<finder-column name="companyId" />
			<finder-column name="name" />
		</finder>
		<finder name="ProcessType_CompanyId" return-type="Collection">
			<finder-column name="companyId" />
			<finder-column name="processType" />
		</finder>
	</entity>
	<entity local-service="true" name="ScheduledTask" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="scheduledTaskId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="name" type="String" />
		<column name="frequency" type="String" />
		<column name="processId" type="long" />
		<column name="status" type="int" />
		<column name="active" type="boolean" />
		<column name="runStartDate" type="Date" />
		<column name="runEndDate" type="Date" />
		<column name="startDate" type="Date" />
		<column name="startHour" type="String" />
		<column name="enabled" type="boolean" />

		<!-- Order -->

		<order by="desc">
			<order-column name="modifiedDate" />
		</order>

		<!-- Finder methods -->

		<finder name="GroupId" return-type="Collection">
			<finder-column name="groupId" />
		</finder>
		<finder name="Active" return-type="Collection">
			<finder-column name="active" />
		</finder>
		<finder name="Enabled" return-type="Collection">
			<finder-column name="enabled" />
		</finder>
		<finder name="GroupId_Active" return-type="Collection">
			<finder-column name="groupId" />
			<finder-column name="active" />
		</finder>
		<finder name="GroupId_Enabled" return-type="Collection">
			<finder-column name="groupId" />
			<finder-column name="enabled" />
		</finder>
		<finder name="Name" return-type="Collection">
			<finder-column name="companyId" />
			<finder-column name="name" />
		</finder>
	</entity>
</service-builder>