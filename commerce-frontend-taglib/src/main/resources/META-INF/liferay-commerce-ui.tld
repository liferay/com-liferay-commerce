<?xml version="1.0"?>

<taglib
	version="2.1"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_2_1.xsd"
>
	<description>Provides the Liferay Commerce UI common tags, prefixed with <![CDATA[<code>commerce-ui:</code>]]>.</description>
	<tlib-version>1.0</tlib-version>
	<short-name>liferay-commerce-ui</short-name>
	<uri>http://liferay.com/tld/commerce-ui</uri>
	<tag>
		<name>account-selector</name>
		<tag-class>
			com.liferay.commerce.frontend.taglib.servlet.taglib.AccountSelectorTag</tag-class>
		<body-content>empty</body-content>
	</tag>
	<tag>
		<name>add-to-cart</name>
		<tag-class>
			com.liferay.commerce.frontend.taglib.servlet.taglib.AddToCartTag</tag-class>
		<body-content>empty</body-content>
	</tag>
	<tag>
		<name>add-to-tick-item</name>
		<tag-class>
			com.liferay.commerce.frontend.taglib.servlet.taglib.AddToTickItemTag</tag-class>
		<body-content>empty</body-content>
	</tag>
	<tag>
		<name>autocomplete-item</name>
		<tag-class>
			com.liferay.commerce.frontend.taglib.servlet.taglib.AutocompleteItemTag</tag-class>
		<body-content>empty</body-content>
	</tag>
	<tag>
		<name>product-details-modal</name>
		<tag-class>
			com.liferay.commerce.frontend.taglib.servlet.taglib.ProductDetailsModalTag</tag-class>
		<body-content>empty</body-content>
	</tag>
	<tag>
		<name>quantity-selector</name>
		<tag-class>
			com.liferay.commerce.frontend.taglib.servlet.taglib.QuantitySelectorTag</tag-class>
		<body-content>empty</body-content>
	</tag>
	<tag>
		<name>search-bar</name>
		<tag-class>com.liferay.commerce.frontend.taglib.servlet.taglib.SearchBarTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>search-results</name>
		<tag-class>com.liferay.commerce.frontend.taglib.servlet.taglib.SearchResultsTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>table</name>
		<tag-class>
			com.liferay.commerce.frontend.taglib.servlet.taglib.CommerceTableTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>dataProviderKey</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>deltaParam</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>disableAJAX</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>boolean</type>
		</attribute>
		<attribute>
			<name>filter</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>com.liferay.commerce.frontend.Filter</type>
		</attribute>
		<attribute>
			<name>itemPerPage</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>int</type>
		</attribute>
		<attribute>
			<name>namespace</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>pageNumber</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>int</type>
		</attribute>
		<attribute>
			<name>portletURL</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>javax.portlet.PortletURL</type>
		</attribute>
		<attribute>
			<name>tableName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>user-invitation-modal</name>
		<tag-class>
			com.liferay.commerce.frontend.taglib.servlet.taglib.UserInvitationModalTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>componentId</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>add-account-modal</name>
		<tag-class>
			com.liferay.commerce.frontend.taglib.servlet.taglib.AddAccountModalTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>componentId</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib>